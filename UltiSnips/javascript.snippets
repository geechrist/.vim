# Generator function
snippet gfn
function* ${1:name}(${2}) {
	yield ${3};
}
endsnippet

# Arrow function
snippet =>
(${1}) => {
	${2}
}
endsnippet

# Class
snippet class
class ${1:name} {
	constructor (${2:arg}) {
		${3:// init}
	}
	${4}
}
endsnippet

# Import
snippet im
import ${1:foo} from '${2:bar}'
endsnippet
